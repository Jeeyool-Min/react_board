{
  "extends": ["airbnb"],
  "rules": {
    "linebreak-style": 0,
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "max-len": "off", //["off", { "code": 220 }],
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off",
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["**/*.config.js", "**/*.config.jsx"] }
    ],
    "react/jsx-props-no-spreading": "off"
    //"no-unused-expressions": ["error", { "allowedTernary": true }]
    // "react/jsx-one-expression-per-line": "off"
  },
  "env": {
    "browser": true
  }
}
